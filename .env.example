# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# Database
DATABASE_URL=sqlite:///./creait.db

# Playwright Settings
HEADLESS=true
BROWSER_TIMEOUT=30000
DEFAULT_NAVIGATION_TIMEOUT=30000

# Logging
LOG_LEVEL=INFO
LOG_FILE=creait.log

# Retry Settings
MAX_RETRIES=3
RETRY_DELAY=2